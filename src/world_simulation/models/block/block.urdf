<robot name="block">
  <static>1</static>
  <link name="block">
    <inertial>
      <origin xyz="0.0 0.0 0.0" />
      <mass value=".01" />
      <!-- (1/12)*mass*(h^2 + d^2)  (1/12)*mass*(w^2 + d^2) (1/12)*mass*(w^2 + h^2) -->
      <!-- (1/12)*0.5*(0.045^2 + 0.045^2)  (1/12)*0.5*(0.045^2 + 0.045^2) (1/12)*0.5*(0.045^2 + 0.045^2) -->
      <inertia  ixx="0.000052083" ixy="0.0"  ixz="0.0"  iyy="0.000052083"  iyz="0.0"  izz="0.000052083" />

    </inertial>
    <visual>
      <origin xyz="0.0 0.0 0.0" />
      <geometry>
        {COLOR_REPLACE_GEOMETRY}
      </geometry>
    </visual>
    <collision>
      <origin xyz="0.0 0.0 0.0" />
      <geometry>
        <box size="0.05 0.05 0.40" />
      </geometry>
      <surface>
        <friction>
          <ode>
            <mu>0.75</mu>
            <mu2>0.75</mu2>
          </ode>
        </friction>
        <contact>
          <ode>
            <kp>1000000.0</kp>
            <kd>1</kd>
            <min_depth>0.0025</min_depth>
            <max_vel>1.0</max_vel>
          </ode>
        </contact>
      </surface>
    </collision>
  </link>

  <link name="handholds">
    <inertial>
      <origin xyz="0.0 0.0 0.0" />
      <mass value=".01" />
      <!-- (1/12)*mass*(h^2 + d^2)  (1/12)*mass*(w^2 + d^2) (1/12)*mass*(w^2 + h^2) -->
      <!-- (1/12)*0.5*(0.045^2 + 0.045^2)  (1/12)*0.5*(0.045^2 + 0.045^2) (1/12)*0.5*(0.045^2 + 0.045^2) -->
      <inertia  ixx="0.000052083" ixy="0.0"  ixz="0.0"  iyy="0.000052083"  iyz="0.0"  izz="0.000052083" />

    </inertial>
    <visual>
      <origin xyz="0.0 0.0 0.0" />
      <geometry>
        <box size="0.1 0.1 0.02" />
      </geometry>
    </visual>
    <collision>
      <origin xyz="0.0 0.0 0.0" />
      <geometry>
        <box size="0.1 0.1 0.02" />
      </geometry>
      <surface>
        <friction>
          <ode>
            <mu>0.75</mu>
            <mu2>0.75</mu2>
          </ode>
        </friction>
        <contact>
          <ode>
            <kp>1000000.0</kp>
            <kd>1</kd>
            <min_depth>0.0025</min_depth>
            <max_vel>1.0</max_vel>
          </ode>
        </contact>
      </surface>
    </collision>
  </link>

  <joint name="block_to_handholds" type="fixed">
    <parent link="block"/>
    <child link="handholds"/>
    <origin xyz="0 0 0.1"/>
  </joint>

   {COLOR_REPLACE_MATERIAL}
</robot>
