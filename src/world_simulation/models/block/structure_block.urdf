<robot name="block">
  <static>1</static>
  <link name="block">
    <inertial>
      <origin xyz="0.0 0.0 0.0" />
      <mass value=".5" />
      <!-- (1/12)*mass*(h^2 + d^2)  (1/12)*mass*(w^2 + d^2) (1/12)*mass*(w^2 + h^2) -->
      <!-- (1/12)*0.5*(0.045^2 + 0.045^2)  (1/12)*0.5*(0.045^2 + 0.045^2) (1/12)*0.5*(0.045^2 + 0.045^2) -->
      <inertia  ixx="0.0" ixy="0.0"  ixz="0.0"  iyy="0.0"  iyz="0.0"  izz="0.0" />
      <geometry>
        <box size="0.05 0.05 0.4" />
      </geometry>
    </inertial>
    <visual>
      <origin xyz="0.0 0.0 0.0" />
      <geometry>
        <box size="0.05 0.05 0.4" />
      </geometry>
    </visual>
    <collision>
      <origin xyz="0.0 0.0 0.0" />
      <geometry>
        <box size="0.05 0.05 0.4" />
        <surface>
          <friction>
            <ode>
              <mu>0.75</mu>
              <mu2>0.75</mu2>
            </ode>
          </friction>
          <contact>
            <ode>
              <kp>1000000.0</kp>
              <kd>1</kd>
              <min_depth>0.000005</min_depth>
              <max_vel>1.0</max_vel>
            </ode>
          </contact>
        </surface>
      </geometry>
    </collision>
  </link>
  <gazebo reference="block">
    <dynamics damping=".7" friction="100.0"/>
    <material>Gazebo/Red</material>
  </gazebo>
</robot>
